{
  "name": "unwanted-visitors",
  "description": "Horrors Creepy Storytelling",
  "platform": "youtube",
  "defaults": {
    "aspectRatio": "16:9",
    "animation": "zoom-in",
    "qualityMode": "high",
    "language": "en",
    "randomImages": true,
    "useChannelPool": true,
    "fallbackToGlobalPool": true,
    "randomMusic": true,
    "useChannelMusicPool": true,
    "subtitle": {
      "style": "minimal",
      "position": "bottom",
      "fontSize": 25,
      "fontColor": "#FFFFFF"
    },
    "audio": {
      "musicVolume": 0.08,
      "musicFadeIn": 2,
      "musicFadeOut": 0.5,
      "voiceFadeIn": 1,
      "voiceFadeOut": 0.5
    },
    "cta": {
      "enabled": false
    },
    "exportFormats": {
      "enabled": false
    },
    "imageCount": 4
  }
}